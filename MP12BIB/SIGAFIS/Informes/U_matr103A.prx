	IF DbSeek(cPesq )
		While (cPesq:=xFilial("SEK")+SEK->EK_NUM ) .And. Empty(cBanco)
			If Alltrim(SEK->EK_TIPO) $ "TF|CH"
				cBanco := SEK->EK_BANCO+SEK->EK_AGENCIA+SEK->EK_CONTA
