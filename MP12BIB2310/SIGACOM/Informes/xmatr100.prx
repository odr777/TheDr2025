USER Function ImprimeSC(lAtiva,nTipo,oSection1)

If lAtiva 
	oSection1:Cell("C1_NUM"):Show()
	oSection1:Cell("C1_ITEM"):Show()
	oSection1:Cell("C1_PRODUTO"):Show()
	oSection1:Cell("C1_DESCRI"):Show()
	oSection1:Cell("B1_TIPO"):Show()
	oSection1:Cell("B1_GRUPO"):Show()
	oSection1:Cell("C1_QUANT"):Show()
	oSection1:Cell("C1_UM"):Show()
	oSection1:Cell("C1_CC"):Show()
	oSection1:Cell("CTT_DESC01"):Show()
	oSection1:Cell("C1_EMISSAO"):Show()
	oSection1:Cell("C1_DATPRF"):Show()
	oSection1:Cell("C1_SOLICIT"):Show()
	
	oSection1:Cell("nSaldo"):Show()

	oSection1:Cell("C1_LOCAL"):Show()
	oSection1:Cell("C1_CLVL"):Show()
	oSection1:Cell("C1_OBS"):Show()
else
	oSection1:Cell("C1_NUM"):Hide()
	oSection1:Cell("C1_ITEM"):Hide()
	oSection1:Cell("C1_PRODUTO"):Hide()
	oSection1:Cell("C1_DESCRI"):Hide()
	oSection1:Cell("B1_TIPO"):Hide()
	oSection1:Cell("B1_GRUPO"):Hide()
	oSection1:Cell("C1_QUANT"):Hide()
	oSection1:Cell("C1_UM"):Hide()
	oSection1:Cell("C1_CC"):Hide()
	oSection1:Cell("CTT_DESC01"):Hide()
	oSection1:Cell("C1_EMISSAO"):Hide()
	oSection1:Cell("C1_DATPRF"):Hide()
	oSection1:Cell("C1_SOLICIT"):Hide()
	//oSection1:Cell("dDtPrazo"):Hide()
	oSection1:Cell("nSaldo"):Hide()

	oSection1:Cell("C1_LOCAL"):Hide()
	oSection1:Cell("C1_CLVL"):Hide()
	oSection1:Cell("C1_OBS"):Hide()
EndIf


Do Case
    Case nTipo == 0 
		If lAtiva
			oSection1:Cell("C1_QUANT"):SetValue(SC1->C1_QUANT)
			oSection1:Cell("nSaldo"):SetValue(SC1->C1_QUANT-SC1->C1_QUJE)
			oSection1:PrintLine()
		Else	
			oSection1:Cell("C1_QUANT"):SetValue(0)
			oSection1:Cell("nSaldo"):SetValue(0)
			oSection1:PrintLine()
		Endif	
    
    Case nTipo == 1
	    If lAtiva

			
			oSection1:Cell("C1_QUANT"):SetValue(SC1->C1_QUANT)
			oSection1:Cell("nSaldo"):SetValue(SC1->C1_QUANT-SC1->C1_QUJE)

			oSection1:PrintLine()
				
			
		EndIf
EndCase

Return 


/**
* @author: Denar Terrazas Parada
* @since: 31/03/2021
* @description: Función que actualiza el valor de las preguntas
*/
Static Function actPreguntas()
	Local aArea:= getArea()
	Local cPerg:= "MTR100"

	SX1->(DbSetOrder(1))
	If SX1->(DbSeek(PADR(alltrim(cPerg), 10)+"01"))
		Reclock("SX1",.F.)
		SX1->X1_CNT01 := SC1->C1_NUM
		SX1->(MsUnlock())
	End

	If SX1->(DbSeek(PADR(alltrim(cPerg), 10)+"02"))
		Reclock("SX1",.F.)
		SX1->X1_CNT01 := SC1->C1_NUM
		SX1->(MsUnlock())
	End

	If SX1->(DbSeek(PADR(alltrim(cPerg), 10)+"03"))
		Reclock("SX1",.F.)
		SX1->X1_PRESEL := 1
		SX1->(MsUnlock())
	End

	If SX1->(DbSeek(PADR(alltrim(cPerg), 10)+"04"))
		Reclock("SX1",.F.)
		SX1->X1_CNT01 := DTOS(SC1->C1_EMISSAO)
		SX1->(MsUnlock())
	End

	If SX1->(DbSeek(PADR(alltrim(cPerg), 10)+"05"))
		Reclock("SX1",.F.)
		SX1->X1_CNT01 := DTOS(SC1->C1_EMISSAO)
		SX1->(MsUnlock())
	End

	If SX1->(DbSeek(PADR(alltrim(cPerg), 10)+"06"))
		Reclock("SX1",.F.)
		SX1->X1_PRESEL := 2
		SX1->(MsUnlock())
	End

	If SX1->(DbSeek(PADR(alltrim(cPerg), 10)+"07"))
		Reclock("SX1",.F.)
		SX1->X1_PRESEL := 3
		SX1->(MsUnlock())
	End

	If SX1->(DbSeek(PADR(alltrim(cPerg), 10)+"08"))
		Reclock("SX1",.F.)
		SX1->X1_PRESEL := 2
		SX1->(MsUnlock())
	End

	restArea(aArea)

Return
