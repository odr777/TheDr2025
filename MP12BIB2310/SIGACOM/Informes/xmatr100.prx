//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³Criacao do componente de impressao                                      ³
//³                                                                        ³
//³TReport():New                                                           ³
//³ExpC1 : Nome do relatorio                                               ³
//³ExpC2 : Titulo                                                          ³
//³ExpC3 : Pergunte                                                        ³
//³ExpB4 : Bloco de codigo que sera executado na confirmacao da impressao  ³
//³ExpC5 : Descricao                                                       ³
//³                                                                        ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
oReport:= TReport():New("XMATR100",STR0005,"MTR100", {|oReport| ReportPrint(oReport)},STR0001+" "+STR0002) //"Relacao de Solicitacoes de Compras"##"Emite um relacao para controle das solicitacoes cadastradas ,"##"seus respectivos pedidos e prazos de entrega."
oReport:SetLandscape()    
oReport:SetTotalInLine(.F.)

//actPreguntas()

Pergunte("MTR100",.F.)

MV_PAR06 := 2  /// NO CONSIDERA PC
MV_PAR01 := SC1->C1_NUM
MV_PAR02 := SC1->C1_NUM
MV_PAR03 := 1
MV_PAR04 := (SC1->C1_EMISSAO) - 30
MV_PAR05 := (SC1->C1_EMISSAO) + (10 * 350)
MV_PAR07 := 3
MV_PAR08 := 2

Aadd( aOrdem, STR0003 ) //" Por Solicitacao    "

oSection1 := TRSection():New(oReport,STR0022,{"SC1","SB1","SC7","SA2"},aOrdem) //"Relacao de Solicitacoes de Compras"
oSection1:SetTotalInLine(.F.)
oSection1:SetHeaderPage()
oSection1:SetNoFilter("SB1")
oSection1:SetNoFilter("SC7")
oSection1:SetNoFilter("SA2")

TRCell():New(oSection1,"C1_NUM"		,"SC1",STR0024+CRLF+STR0025/*Titulo*/	,/*Picture*/,TamSX3("C1_NUM")[1]+1,/*lPixel*/,/*{|| code-block de impressao }*/) // "Numero"##"SC"
TRCell():New(oSection1,"C1_ITEM"	,"SC1",STR0026+CRLF+STR0025/*Titulo*/	,/*Picture*/,TamSX3("C1_ITEM")[1]+1,/*lPixel*/,/*{|| code-block de impressao }*/) // "Item"##"SC"
TRCell():New(oSection1,"C1_PRODUTO"	,"SC1",/*Titulo*/						,/*Picture*/,TamSX3("C1_PRODUTO")[1]+7,/*lPixel*/,/*{|| code-block de impressao }*/,/**/, /**/, /**/, /**/, /**/,.F.)
TRCell():New(oSection1,"C1_DESCRI"	,"SC1",/*Titulo*/						,/*Picture*/,40			,/*lPixel*/,/*{|| code-block de impressao }*/)
TRCell():New(oSection1,"B1_TIPO"	,"SB1",STR0027/*Titulo*/				,/*Picture*/,/*Tamanho*/,/*lPixel*/,/*{|| code-block de impressao }*/) //"Tp"
TRCell():New(oSection1,"B1_GRUPO"	,"SB1",/*Titulo*/						,/*Picture*/,/*Tamanho*/,/*lPixel*/,/*{|| code-block de impressao }*/)
TRCell():New(oSection1,"C1_QUANT"	,"SC1",/*Titulo*/						,/*Picture*/,/*Tamanho*/,/*lPixel*/,/*{|| code-block de impressao }*/)
TRCell():New(oSection1,"C1_UM"		,"SC1",STR0028/*Titulo*/				,/*Picture*/,/*Tamanho*/,/*lPixel*/,/*{|| code-block de impressao }*/) //"UM"
TRCell():New(oSection1,"C1_CC"		,"SC1",/*Titulo*/						,/*Picture*/,20			,/*lPixel*/,/*{|| code-block de impressao }*/)
TRCell():New(oSection1,"CTT_DESC01"	,"CTT","Desc. C.C."						,/*Picture*/,20/*Tamanho*/,/*lPixel*/,/*{|| code-block de impressao }*/,,.T.)
TRCell():New(oSection1,"C1_EMISSAO"	,"SC1",STR0029+CRLF+STR0030/*Titulo*/	,/*Picture*/,/*Tamanho*/,/*lPixel*/,/*{|| code-block de impressao }*/) //"Emissao"
TRCell():New(oSection1,"C1_DATPRF"	,"SC1","Necesidad"/*Titulo*/	,/*Picture*/,/*Tamanho*/,/*lPixel*/,/*{|| code-block de impressao }*/) //"Entrega"##"SC"
TRCell():New(oSection1,"C1_SOLICIT"	,"SC1",/*Titulo*/						,/*Picture*/,9			,/*lPixel*/,/*{|| code-block de impressao }*/)

TRCell():New(oSection1,"nSaldo"		,"   ",STR0020,/*Picture*/,/*Tamanho*/	,/*lPixel*/,/*{|| code-block de impressao }*/) //"Saldo SC"
TRCell():New(oSection1,"C1_LOCAL"		,"   ","Deposito",/*Picture*/,/*Tamanho*/	,/*lPixel*/,/*{|| code-block de impressao }*/) //"Saldo SC"
TRCell():New(oSection1,"C1_CLVL"		,"   ","Clase Valor",/*Picture*/,/*Tamanho*/	,/*lPixel*/,/*{|| code-block de impressao }*/) //"Saldo SC"
TRCell():New(oSection1,"C1_OBS"		,"   ","Observacion",/*Picture*/,20/*Tamanho*/	,/*lPixel*/,/*{|| code-block de impressao }*/,,.T.) //"Saldo SC"

Return(oReport)

