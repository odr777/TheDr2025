	LOCAL cDesc1   := STR0002	//"Lista  as  movimentacoes  internas  da empresa  ,ou seja ,Requisicoes ,"
	LOCAL cDesc2   := STR0003	//"Devolucoes ,Producoes e Estornos de Producao."
